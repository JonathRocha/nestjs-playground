{
    "name": "boilerplate",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nodemon src/main.ts",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/mysql.ts",
        "typeorm:migrate": "yarn run typeorm migration:generate -n",
        "typeorm:run": "yarn run typeorm migration:run",
        "prisma:generate": "MYSQL_INTRANET_URL= && yarn prisma2 generate"
    },
    "dependencies": {
        "@codeinplace/js-utils": "^1.0.1",
        "@nestjs/common": "^6.11.7",
        "@nestjs/core": "^6.11.7",
        "@nestjs/jwt": "^6.1.1",
        "@nestjs/passport": "^6.1.1",
        "@nestjs/platform-express": "^6.11.7",
        "@nestjs/sequelize": "^0.0.2",
        "@nestjs/typeorm": "^6.3.1",
        "@prisma/client": "^2.0.0-preview022",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.14.0",
        "compression": "^1.7.4",
        "dotenv": "^8.2.0",
        "express-rate-limit": "^5.1.1",
        "helmet": "^3.21.2",
        "mongodb": "^3.5.3",
        "mongoose": "^5.9.1",
        "mysql": "^2.18.1",
        "mysql2": "^2.1.0",
        "nest-router": "^1.0.9",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "prisma2": "^2.0.0-preview022",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.4",
        "sequelize": "^5.21.4",
        "sequelize-typescript": "^1.1.0",
        "typeorm": "^0.2.22"
    },
    "devDependencies": {
        "@nestjs/cli": "^6.14.2",
        "@nestjs/schematics": "^6.9.4",
        "@nestjs/testing": "^6.11.7",
        "@types/compression": "^1.7.0",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.2",
        "@types/express-rate-limit": "^3.3.3",
        "@types/helmet": "^0.0.45",
        "@types/jest": "25.1.2",
        "@types/mongodb": "^3.3.16",
        "@types/mongoose": "^5.7.1",
        "@types/mysql": "^2.15.8",
        "@types/node": "^13.7.1",
        "@types/passport-jwt": "^3.0.3",
        "@types/sequelize": "^4.28.8",
        "@types/supertest": "^2.0.8",
        "@typescript-eslint/eslint-plugin": "^2.20.0",
        "@typescript-eslint/parser": "^2.20.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-prettier": "^3.1.2",
        "jest": "^25.1.0",
        "nodemon": "^2.0.2",
        "prettier": "^1.19.1",
        "supertest": "^4.0.2",
        "ts-jest": "25.2.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.7.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
